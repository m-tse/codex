{
  "[go]": {
    "editor.insertSpaces": false,
    "editor.tabSize": 2
  },
  "[python]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4
  },
  "editor.acceptSuggestionOnEnter": "off",
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.copyWithSyntaxHighlighting": false,
  "editor.cursorBlinking": "phase",
  "editor.cursorSurroundingLines": 15,
  "editor.cursorWidth": 4,
  "editor.dragAndDrop": false,
  "editor.fontFamily": "SF Mono, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 13.5,
  "window.zoomLevel": -1,
  "editor.formatOnPaste": true,
  "editor.hover.delay": 800,
  "editor.minimap.enabled": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.padding.bottom": 10,
  "editor.padding.top": 10,
  "editor.quickSuggestionsDelay": 0,
  "editor.renderLineHighlight": "all",
  "editor.renderLineHighlightOnlyWhenFocus": true,
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [80, 100, 120],
  "editor.scrollbar.verticalScrollbarSize": 15,
  "editor.smoothScrolling": true,
  "editor.suggest.localityBonus": true,
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "editor.wrappingIndent": "deepIndent",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 0,
  "explorer.sortOrder": "foldersNestsFiles",
  "extensions.ignoreRecommendations": true,
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 100,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "gitlens.codeLens.enabled": false,
  "gitlens.showWelcomeOnInstall": false,
  "search.seedWithNearestWord": true,
  "search.smartCase": true,
  "search.sortOrder": "countDescending",
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.automationProfile.windows": {},
  "window.nativeFullScreen": false,
  "window.title": "${activeEditorMedium}${separator}${rootName}${separator}${profileName}",
  "window.titleBarStyle": "native",
  "workbench.commandPalette.history": 5,
  "workbench.editor.closeEmptyGroups": false,
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.perEditorGroup": true,
  "workbench.editor.limit.value": 4,
  "workbench.editor.openPositioning": "first",
  "workbench.editor.revealIfOpen": true,
  "workbench.editor.tabCloseButton": "off",
  "workbench.enableExperiments": false,
  "workbench.fontAliasing": "auto",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.tips.enabled": false,
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": true,
    "scminput": false
  },
  "editor.formatOnSave": true,
  "workbench.activityBar.location": "hidden",
  "workbench.editor.empty.hint": "hidden",
  "workbench.editor.tabActionCloseVisibility": false,
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "scm.diffDecorationsGutterWidth": 4,
  // Turn off all auto suggestions in favor of github copilot
  "editor.suggest.showColors": false,
  "editor.suggest.showCustomcolors": false,
  "editor.suggest.showDeprecated": false,
  "editor.suggest.showEnumMembers": false,
  "editor.suggest.showEnums": false,
  "editor.suggest.showEvents": false,
  "editor.suggest.showFields": false,
  "editor.suggest.showFiles": false,
  "editor.suggest.showFolders": false,
  "editor.suggest.showIcons": false,
  "editor.suggest.showInlineDetails": false,
  "editor.suggest.showInterfaces": false,
  "editor.suggest.showIssues": false,
  "editor.suggest.showOperators": false,
  "editor.suggest.showProperties": false,
  "editor.suggest.showSnippets": false,
  "editor.suggest.showStructs": false,
  "editor.suggest.showTypeParameters": false,
  "editor.suggest.showUnits": false,
  "editor.suggest.showUsers": false,
  "editor.suggest.showValues": false,
  "editor.suggest.showClasses": false,
  "editor.suggest.showConstants": false,
  "editor.suggest.showConstructors": false,
  "editor.suggest.showFunctions": false,
  "editor.suggest.showKeywords": false,
  "editor.suggest.showMethods": false,
  "editor.suggest.showModules": false,
  "editor.suggest.showReferences": false,
  "editor.suggest.showVariables": false,
  "editor.suggest.showWords": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
}
