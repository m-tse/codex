{
  "[go]": {
    "editor.insertSpaces": false,
    "editor.tabSize": 2
    },
  "[python]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4
    },
  "editor.acceptSuggestionOnEnter": "off",
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.copyWithSyntaxHighlighting": false,
  "editor.cursorBlinking": "phase",
  "editor.cursorSurroundingLines": 15,
  "editor.cursorWidth": 4,
  "editor.dragAndDrop": false,
  "editor.fontFamily": "SF Mono, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 13.5,
  "editor.formatOnPaste": true,
  "editor.hover.delay": 800,
  "editor.minimap.enabled": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.padding.bottom": 10,
  "editor.padding.top": 10,
  "editor.quickSuggestionsDelay": 500,
  "editor.renderLineHighlight": "all",
  "editor.renderLineHighlightOnlyWhenFocus": true,
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [
    80,
    100,
    120
  ],
  "editor.scrollbar.verticalScrollbarSize": 15,
  "editor.smoothScrolling": true,
  "editor.suggest.localityBonus": true,
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "editor.wrappingIndent": "deepIndent",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 0,
  "explorer.sortOrder": "foldersNestsFiles",
  "extensions.ignoreRecommendations": true,
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 100,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "gitlens.codeLens.enabled": false,
  "gitlens.showWelcomeOnInstall": false,
  "search.seedWithNearestWord": true,
  "search.smartCase": true,
  "search.sortOrder": "countDescending",
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.automationProfile.windows": {},
  "window.nativeFullScreen": false,
  "window.title": "${activeEditorMedium}${separator}${rootName}${separator}${profileName}",
  "window.titleBarStyle": "native",
  "window.zoomLevel": -1.5,
  "workbench.activityBar.visible": false,
  "workbench.commandPalette.history": 5,
  "workbench.editor.closeEmptyGroups": false,
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.perEditorGroup": true,
  "workbench.editor.limit.value": 1,
  "workbench.editor.openPositioning": "first",
  "workbench.editor.revealIfOpen": true,
  "workbench.editor.tabCloseButton": "off",
  "workbench.editor.untitled.hint": "hidden",
  "workbench.enableExperiments": false,
  "workbench.fontAliasing": "auto",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.tips.enabled": false
}
